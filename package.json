{
  "name": "nodejs-express-react-event-registration-system",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack -p",
    "build:server": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "concurrently -k \"webpack --watch\" \"nodemon ./bin/www --watch build\""
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.11.5",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pug": "^2.0.0-rc.3",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "cross-env": "^5.0.3",
    "css-loader": "^0.28.4",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-datetime": "^2.9.0",
    "react-dom": "^15.6.1",
    "react-refetch": "^1.0.0",
    "react-router": "^3.0.5",
    "react-select2-wrapper": "^1.0.4-beta5",
    "react-spinkit": "^3.0.0",
    "spicy-datatable": "^0.4.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1"
  }
}
